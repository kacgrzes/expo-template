{
  "name": "expo-template",
  "version": "1.0.0",
  "main": "src/index.tsx",
  "description": "Expo template ready to develop mobile apps",
  "keywords": [
    "expo",
    "expo-template",
    "react-native",
    "react-native-template",
    "template"
  ],
  "license": "MIT",
  "author": "Kacper Grzeszczyk",
  "repository": {
    "type": "git",
    "url": "https://github.com/kacgrzes/expo-template"
  },
  "bugs": {
    "url": "https://github.com/kacgrzes/expo-template/issues"
  },
  "scripts": {
    "analyzeAndroid": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'",
    "analyzeIos": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "analyzeWeb": "source-map-explorer 'dist/_expo/static/js/web/*.js' 'dist/_expo/static/js/web/*.js.map'",
    "android": "expo run:android",
    "appIcon": "npx app-icon init",
    "atlas": "npx expo-atlas .expo/atlas.jsonl",
    "check": "npm run lint && npm run tsCheck",
    "checkExpo": "npm run doctor && npm expo install --check && npm outdated",
    "clean": "rm -rf dist && rm -rf .expo && npm run stopWatchman && rm -rf ios/ && rm -rf android/ && rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf ./node-modules/",
    "cleanGit": "git clean -xfd",
    "compilerHealthcheck": "npx react-compiler-healthcheck",
    "config": "npx expo config",
    "deeplink": "npx uri-scheme open --ios template://profile",
    "devtools": "npx react-devtools",
    "doctor": "npx expo-doctor && npm run compilerHealthcheck",
    "easBuildAndroidDevelopment": "eas build --platform android --profile development",
    "easBuildAndroidPreview": "eas build --platform android --profile preview",
    "easBuildIosDevelopmentDevice": "eas build --platform ios --profile development",
    "easBuildIosDevelopmentSimulator": "eas build --platform ios --profile ios-simulator",
    "easBuildIosPreview": "eas build --platform ios --profile preview",
    "easDeviceCreate": "eas device:create",
    "exportSourceMapsAll": "EXPO_UNSTABLE_ATLAS=true npx expo export --source-maps --platform all",
    "exportSourceMapsAndroid": "EXPO_UNSTABLE_ATLAS=true npx expo export --source-maps --platform android --no-bytecode",
    "exportSourceMapsIos": "EXPO_UNSTABLE_ATLAS=true npx expo export --source-maps --platform ios --no-bytecode",
    "exportSourceMapsWeb": "EXPO_UNSTABLE_ATLAS=true npx expo export --source-maps --platform web",
    "generate": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile=plopfile.ts",
    "installEAS": "npm install -g eas-cli",
    "installOrbit": "brew install expo-orbit",
    "ios": "npm run runIosDevelopment",
    "iosDevice": "npm run runIosDevelopment -- --device",
    "lint": "eslint .",
    "listSchemes": "npx uri-scheme list",
    "logAndroid": "npx react-native log-android",
    "logIos": "npx react-native log-ios",
    "openInXcode": "xed ios",
    "postinstall": "patch-package && npm run prebuild",
    "prebuild": "expo prebuild --clean",
    "prebuildAndroid": "expo prebuild --clean --platform ios",
    "prebuildIos": "npm run prebuildIosDevelopment",
    "prebuildIosDevelopment": "npx cross-env APP_VARIANT=development expo prebuild --clean --platform ios",
    "prebuildIosPreview": "npx cross-env APP_VARIANT=preview expo prebuild --clean --platform ios",
    "prebuildIosProduction": "expo prebuild --clean --platform ios",
    "prepare": "husky",
    "prepareI18next": "i18next-resources-for-ts interface -i ./modules/i18n/locales -o ./@types/resources.d.ts",
    "prettierWrite": "npx prettier . --write",
    "restartWatchman": "npm run stopWatchman && npm run startWatchman",
    "runAndroidDevelopment": "npx cross-env APP_VARIANT=development expo run:android",
    "runAndroidPreview": "npx cross-env APP_VARIANT=preview expo run:android --variant release",
    "runAndroidProduction": "expo run:ios --variant release",
    "runIosDevelopment": "npx cross-env APP_VARIANT=development expo run:ios",
    "runIosPreview": "npx cross-env APP_VARIANT=preview expo run:ios --configuration Release",
    "runIosProduction": "expo run:ios --configuration Release",
    "start": "npm run startDevelopment",
    "startDevelopment": "npx cross-env EXPO_USE_FAST_RESOLVER=1 APP_VARIANT=development npm run restartWatchman && expo start",
    "startPreview": "APP_VARIANT=preview npm run startProduction",
    "startProduction": "expo start --no-dev --minify",
    "startProductionNoMinify": "expo start --no-dev --no-minify",
    "startWatchman": "watchman watch-project $(pwd)",
    "startWithAtlas": "npx cross-env EXPO_UNSTABLE_ATLAS=true APP_VARIANT=development expo start",
    "stopWatchman": "watchman watch-del $(pwd)",
    "storybookGenerate": "sb-rn-get-stories",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest --passWithNoTests",
    "tsCheck": "tsc",
    "updateExpo": "npm install expo@latest && npx expo install --fix && npm run doctor && npm outdated",
    "updateSnapshots": "jest -u --coverage=false",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo-google-fonts/ibm-plex-mono": "^0.2.3",

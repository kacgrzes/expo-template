{
  "name": "expo-template",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "analyze:android": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'",
    "analyze:ios": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "analyze:web": "source-map-explorer 'dist/_expo/static/js/web/*.js' 'dist/_expo/static/js/web/*.js.map'",
    "android": "expo run:android",
    "clean": "rm -rf dist && rm -rf .expo && watchman watch-del-all && rm -rf ios/ && rm -rf android/ && rm -rf /Users/kacgrzes/Library/Developer/Xcode/DerivedData && rm -rf ./node-modules/",
    "deeplink": "uri-scheme open --ios expo-template://index",
    "devtools": "npx react-devtools",
    "doctor": "npx expo-doctor",
    "export-source-maps:android": "npx expo export --source-maps --platform android --no-bytecode",
    "export-source-maps:ios": "npx expo export --source-maps --platform ios --no-bytecode",
    "export-source-maps:web": "npx expo export --source-maps --platform web",
    "generate": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile=plopfile.ts",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "lint": "eslint .",
    "log:android": "npx react-native log-android",
    "log:ios": "npx react-native log-ios",
    "prebuild": "expo prebuild --clean",
    "prebuild:android": "expo prebuild --clean --platform ios",
    "prebuild:ios": "expo prebuild --clean --platform ios",
    "prepare:i18next": "i18next-resources-for-ts interface -i ./i18n/locales/en -o ./@types/resources.d.ts",
    "start": "EXPO_USE_FAST_RESOLVER=1 && expo start",
    "start:production": "expo start --no-dev --minify",
    "storybook-generate": "sb-rn-get-stories",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "test:debug": "jest -o --watch --coverage=false",
    "test:final": "jest",
    "updateExpo": "npm install expo@latest && npx expo install --fix && npm run doctor",
    "updateSnapshots": "jest -u --coverage=false",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo-google-fonts/ibm-plex-mono": "^0.2.3",
    "@expo-google-fonts/ibm-plex-sans": "^0.2.3",
    "@expo-google-fonts/ibm-plex-serif": "^0.2.3",
    "@gorhom/bottom-sheet": "^5.0.0-alpha.9",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "7.7.0",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-community/slider": "4.5.2",
    "@react-native-menu/menu": "^1.0.1",
    "@th3rdwave/react-navigation-bottom-sheet": "^0.2.7",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "expo": "^51.0.0",
    "expo-application": "~5.9.1",
    "expo-asset": "~10.0.6",
    "expo-build-properties": "~0.12.1",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.1",
    "expo-dev-client": "~4.0.11",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.4",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.12.8",
    "expo-keep-awake": "~13.0.1",
    "expo-linking": "~6.3.1",
    "expo-local-authentication": "~14.0.1",
    "expo-localization": "~15.0.3",
    "expo-quick-actions": "^1.0.1",
    "expo-router": "~3.5.10",
    "expo-screen-orientation": "~7.0.3",
    "expo-secure-store": "~13.0.1",
    "expo-sensors": "~13.0.5",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-store-review": "~7.0.2",
    "expo-system-ui": "~3.0.4",
    "expo-tracking-transparency": "~4.0.2",
    "i18next": "^23.11.3",
    "intl-pluralrules": "^2.0.1",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.3",
    "react-i18next": "^14.1.1",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-ios-context-menu": "^2.5.1",
    "react-native-ios-utilities": "^4.4.5",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "3.10.0",

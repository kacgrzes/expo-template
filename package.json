{
  "name": "expo-template",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "analyze:android": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'",
    "analyze:ios": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "analyze:web": "source-map-explorer 'dist/_expo/static/js/web/*.js' 'dist/_expo/static/js/web/*.js.map'",
    "android": "expo run:android",
    "clean": "rm -rf dist && rm -rf .expo && watchman watch-del-all && rm -rf ios/ && rm -rf android/",
    "deeplink": "uri-scheme open --ios expo-template://index",
    "export-source-maps:android": "npx expo export --source-maps --platform android --no-bytecode",
    "export-source-maps:ios": "npx expo export --source-maps --platform ios --no-bytecode",
    "export-source-maps:web": "npx expo export --source-maps --platform web",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "plop": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile=plopfile.ts",
    "prebuild": "expo prebuild --clean",
    "prebuild:ios": "expo prebuild --clean --platform ios",
    "prebuild:android": "expo prebuild --clean --platform ios",
    "run:ios": "expo run:ios",
    "start": "expo start",
    "web": "expo start --web"
  },
  "dependencies": {
    "expo": "^50.0.15",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.11",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-unistyles": "^2.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "source-map-explorer": "^2.5.3",
    "tsx": "^4.7.2",
    "typescript": "^5.3.0",
    "uri-scheme": "^1.2.0"
  },
  "private": true
}
